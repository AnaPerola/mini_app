<h1 class='p-2'>List of Users</h1>

<table class='table'>
  <thead>
    <tr>
      <th class='p-1'>Nickname</th>
      <th class='p-1'>Email</th>
      <th class='p-1'>Role</th>
      <th class='p-1'>Created_at</th>
      <th class='p-1'>Updated_at</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %> 
      <tr>
        <td class='p-1'><%= user.profile.nickname %></td>
        <td class='p-1'><%= user.email %></td>
        <td class='p-1'><%= user.role %></td>
        <td class='p-1'><%= user.created_at %></td>
        <td class='p-1'><%= user.updated_at %></td>
      </tr>
    <% end%>
  </tbody>
</table>


